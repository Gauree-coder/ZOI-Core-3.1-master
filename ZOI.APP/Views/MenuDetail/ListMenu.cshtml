@model IEnumerable<ZOI.APP.Models.MenuDetail>
<h1>List Menu</h1>
<div class="container row p-0 m-0">
    <div class="col-9">
    
    </div>
    <div class="col-3">
        <a asp-action="Upsert" asp-controller="MenuDetail" class="btn btn-info form-control text-white">Add New Menu</a>
    </div>
</div>
<div class="col-12" border p-3>
